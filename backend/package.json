{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "set NODE_ENV=production && node dist/index.js",
    "lint": "echo \"No lint configurado\"",
    "-----Prisma-----": "-----------------------------------------",
    "prismaProvider": "tsx scripts/prismaProvider.ts",
    "prismaInit": "npx prisma migrate dev --name init",
    "prismaReset": "npx prisma migrate reset",
    "prismaMigrate": "npx prisma migrate",
    "prismaMigrateOnli": "npx prisma migrate --create-only",
    "prismaGenerate": "npx prisma generate"
  },
  "dependencies": {
    "fastify": "^5.0.0",
    "@fastify/http-proxy": "^11.3.0",
    "@fastify/static": "^7.0.0",
    "@prisma/client": "^7.1.0",
    "prisma": "^7.1.0",
    "@prisma/adapter-better-sqlite3": "^7.1.0",
    "@prisma/adapter-pg": "^7.1.0",
    "dotenv": "^17.2.3",
    "colors": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "@types/better-sqlite3": "^7.6.13",
    "tsc-alias": "^1.8.16"
  }
}